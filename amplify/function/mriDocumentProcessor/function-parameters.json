{
  "runtimes": [
    "nodejs12.x",
    "python3.8"
  ],
  "permissions": {
    "storage": {
      "Claimaint:@model(appsync)": [
        "read",
        "update"
      ]
    }
  },
  "lambdaLayers": [],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "perPagePrice",
      "environmentVariableName": "PER_PAGE_PRICE"
    },
    {
      "cloudFormationParameterName": "openaiKey",
      "environmentVariableName": "OPENAI_KEY"
    },
    {
      "cloudFormationParameterName": "apiMriReportcosttableName",
      "environmentVariableName": "API_MRI_REPORTCOSTTABLE_NAME"
    },
    {
      "cloudFormationParameterName": "cognitoUserPool",
      "environmentVariableName": "COGNITO_USER_POOL"
    },
    {
      "cloudFormationParameterName": "stripeKey",
      "environmentVariableName": "STRIPE_KEY"
    },
    {
      "cloudFormationParameterName": "stripeCustomerTableName",
      "environmentVariableName": "STRIPE_CUSTOMER_TABLE_NAME"
    }
  ]
}